@model IEnumerable<electo.Models.SP_Models.StoredProcedureModels.sp_getAllRelationshipsInCampaignByUserType_Result_>

<table class="table table-striped">
    <thead>
        <tr>
            <th><img src="~/images/arrow.jpg" width="27" height="34"></th>
            <th>
               Volunteer Name
            </th>
            <th>
                Booth Name/Section Name
            </th>
            <th>
                Address
            </th>           

        </tr>
    </thead>
    @{
        var a = Model.Count();

    }
    <tbody>
        @if (a == 0)
        {
            <tr>
                <td colspan="8">
                    No Result Found
                </td>
            </tr>
        }
        @{    int i = 0; }
        @foreach (var item in Model)
        {

            <tr>
                <td>@(i += 1)</td>
                <td>
                    @Html.DisplayFor(modelItem => item.voterName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.WorkingArea)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.address)
                </td>             
                
            </tr>
        }
    </tbody>
</table>
