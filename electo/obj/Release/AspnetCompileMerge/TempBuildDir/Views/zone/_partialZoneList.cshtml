@model IEnumerable<electo.Models.sp_getZoneByStateIDAndMUncipalCorporationID_Result>


<table class="table table-striped">
    <thead>  <tr>
                 <th><img src="~/images/arrow.jpg" width="27" height="34"></th>
        <th>
            zone
        </th>
        <th>
           Municipal Corp.
        </th>
        <th>
            District 
        </th>       
       
        <th>
            State
        </th>
        
    </tr>
    </thead>
    @{
        var a = Model.Count();

    }
    <tbody>
        @if (a == 0)
        {
            <tr>
                <td colspan="5">
                    No Result Found
                </td>
            </tr>
        }
        @{    int i = 0; }
        @foreach (var item in Model)
        {
            <tr>
                <td>@(i += 1)</td>
                <td>
                    @Html.DisplayFor(modelItem => item.zoneName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.municipalCorporationName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.districtName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.stateName)
                </td>

            </tr>
        }
</tbody>
</table>
