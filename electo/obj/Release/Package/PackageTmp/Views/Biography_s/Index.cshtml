    @{
        ViewBag.Title = "Index";
        Layout = "~/Views/Shared/_LayoutInternal.cshtml";
    }

    <h2>Index</h2>

    <p class="create-btn">
        @Html.ActionLink("Create New", "Create", "", new { @class = "create-new" })
    </p>
    <div class="col-md-9 box-2 running-campaigns sidebar-right">
        <div class="col-md-3">
            <div class="form-group">
                <select class="form-control" id="ddl_Letter">
                    <option value="A">A</option>
                    <option value="B">B</option>
                    <option value="C">C</option>
                    <option value="D">D</option>
                    <option value="E">E</option>
                    <option value="F">F</option>
                    <option value="G">G</option>
                    <option value="H">H</option>
                    <option value="I">I</option>
                    <option value="J">J</option>
                    <option value="K">K</option>
                    <option value="L">L</option>
                    <option value="M">M</option>
                    <option value="N">N</option>
                    <option value="O">O</option>
                    <option value="P">P</option>
                    <option value="Q">Q</option>
                    <option value="R">R</option>
                    <option value="S">S</option>
                    <option value="T">T</option>
                    <option value="U">U</option>
                    <option value="V">V</option>
                    <option value="X">X</option>
                    <option value="W">W</option>
                    <option value="Y">Y</option>
                    <option value="Z">Z</option>
                </select>
            </div>
        </div>
        <div class="col-md-1">
            <div class="form-group">
                <input type="button" name="search" class="btn btn-defaul" value="Search" onclick="return searchResult();" />
            </div>
        </div>
        <div class="table-responsive col-md-12" id="viewResult">
            <div class="col-md-12">
                <p>Please use above search criteria to view the result list</p>
            </div>
        </div>
    </div>

    <script>

        function searchResult() {
            debugger
            var Letter = $("#ddl_Letter").val();


            if (Letter == "") {
                bootbox.alert({
                    title: "Alert !",
                    message: '<p>Please select a letter/charecter from list </p>',
                    timeOut: 2000

                });
                return;
            }
            debugger
            $("#loading").show();
            var stateId = $("#States").val();
            $.ajax({
                url: '/Biography_s/_partialBiography',
                type: 'POST',
                data: { 'Variable_': Letter },
                success: function (data) {
                    debugger
                    $('#viewResult').html(data);
                    $('#loading').hide();
                }
            });
        }
    </script>
