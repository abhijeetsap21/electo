@model IEnumerable<electo.Models.municipalCorporationName>


<table class="table table-striped">
    <thead> <tr>
                <th><img src="~/images/arrow.jpg" width="27" height="34"></th>
        <th>
            @Html.DisplayNameFor(model => model.municipalCorporationName1)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.district.districtName)
        </th>      
      
       
    </tr>
    </thead>
    @{
        var a = Model.Count();

    }
    <tbody>

        @if (a == 0)
        {
            <tr>
                <td colspan="3">
                    No Result Found
                </td>
            </tr>
        }
        @{    int i = 0; }
        @foreach (var item in Model)
        {
            <tr>
                <td>@(i += 1)</td>

                <td>
                    @Html.DisplayFor(modelItem => item.municipalCorporationName1)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.district.districtName)
                </td>


            </tr>
        }
</tbody>
</table>
