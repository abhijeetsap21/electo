
@model IEnumerable<electo.Models.sp_getWardByStateIDAndMUncipalCorporationID_Result>


<table class="table">
    <thead>  <tr>
        <th>
            @Html.DisplayNameFor(model => model.wardNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.wardName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.zoneName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.municipalCorporationName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.districtName)
        </th>

    </tr>
    </thead>
    @{
        var a = Model.Count();

    }
    <tbody>
        @if (a == 0)
        {
            <tr>
                <td>
                    No Result Found
                </td>
            </tr>
        }
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.wardNo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.wardName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.zoneName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.municipalCorporationName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.districtName)
                </td>

            </tr>
        }
</tbody>
</table>
