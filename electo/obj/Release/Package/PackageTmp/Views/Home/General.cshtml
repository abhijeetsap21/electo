@{
    ViewBag.Title = "Home Page";
}

 

   <div class="form-horizontal pageSetup">
        <div class="row">
       
             <div class="col-sm-7 col-sm-offset-1"> 
        <p>
            @if (Model != null)
            {
                @Html.Raw(Model.error_message);
            }
            else
            {
                Exception exception = Server.GetLastError();
                if (exception != null)
                {
                    if (exception.InnerException != null)
                    {
                        @Html.Raw(exception.InnerException.ToString())
                    }
                }
            }
        </p>
